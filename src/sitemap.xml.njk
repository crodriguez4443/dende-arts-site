---
permalink: /sitemap.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {%- for page in collections.all %}
  {%- if not page.data.eleventyExcludeFromCollections %}
  <url>
    <loc>https://dendearts.com{{ page.url }}</loc>
    {%- if page.date %}
    <lastmod>{{ page.date | htmlDateString }}</lastmod>
    {%- endif %}
    {%- if page.data.priority %}
    <priority>{{ page.data.priority }}</priority>
    {%- elif page.url == "/" %}
    <priority>1.0</priority>
    {%- elif page.url == "/blog/" or page.url == "/songbook/" or page.url.startsWith("/abada-joggers") or page.url == "/bulk-orders/" %}
    <priority>0.8</priority>
    {%- elif page.url.startsWith("/blog/") and not page.url.includes("/page-") %}
    <priority>0.7</priority>
    {%- elif page.url.startsWith("/songbook/") %}
    <priority>0.7</priority>
    {%- elif page.url.includes("/page-") %}
    <priority>0.4</priority>
    {%- else %}
    <priority>0.5</priority>
    {%- endif %}
    <changefreq>monthly</changefreq>
  </url>
  {%- endif %}
  {%- endfor %}
</urlset>